{
    "version": 2,
    "builds": [
        {
            "src": "apps/web/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://yobs-api.railway.app/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "apps/web/$1"
        }
    ]
}